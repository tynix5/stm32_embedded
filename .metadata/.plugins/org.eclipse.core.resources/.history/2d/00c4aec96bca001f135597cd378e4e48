#include "main.h"


void clock_config();
void led_config();
void toggle_led();

int main(void) {

	clock_config();
	led_config();

	while (1) {

		toggle_led();
	}

	return 0;
}


void clock_config() {

	RCC->AHB1ENR |= (1 << 0);		// enable GPOIA
}

void led_config() {

	GPIOA->MODER |= (1 << 20);		// output on D2
	GPIOA->MODER &= ~(1 << 21);
}

void toggle_led() {

	GPIOA->ODR ^= (1 << 10);
}
